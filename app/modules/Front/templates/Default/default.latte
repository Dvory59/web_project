<!-- OUR TEAM -->
<section class="home-team">
	<div class="container">
		<!-- section title -->
		<div class="row justify-content-center">
			<div class="col-md-8">
				<div class="sectionTitle text-center">
					<h2>Team</h2>
					<p></p>
				</div>
			</div>
		</div>
		<!-- section title ends -->


		<div  class="row">
			{foreach $team as $profile}
			<div style ="margin:auto;" class="col-md-6 col-lg-3">
				<div class="card card-style2 team-card">
					<div class="card_img">
						<img class="img-full" data-original="{$basePath}/images/profile/avatars/{$profile->avatar}" alt="Card image" style="height:235.5px;">

					</div>
					<div class="card-block">
						<a n:href =":Front:Team:profile, $profile->id"><h4 class="card-title">{$profile->firstName} {$profile->lastName}</h4></a>
						<span class="">{$profile->job_title}</span>
					</div>
				</div>
			</div>
			{/foreach}
		</div>


	</div>
</section>

