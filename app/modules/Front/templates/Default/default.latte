{block head}<link rel="stylesheet" href="{$basePath}/frontend/css/defaultPage.css"> {/block}
{block footer}{/block}
{block content}
<div class="main-wrapper home">
	<!-- BANNER SLIDER CAROUSEL -->
	<section class="banner">
		<div id="banner-slider" class="element_row">
			<!-- slider one -->

			{foreach $sliderArticles as $article}

			<div class="item ">
				<div class="bg-image-holder" style="background-image: url('{$basePath}/images/posts/title/{$article->title_img}');">
					<div class="slider-caption container">
						<div class="caption-content">
							<h1 class="bold text-white" data-animation="customFadeInUp" data-delay="0.1s">{$article->title}</h1>
							<p class="text-white" data-animation="customFadeInUp" data-delay="0.4s">{$article->description|truncate:50}</p>
							<a  class="bold text-white" n:href=":Front:News:detail $article->id"><span  data-delay="0.4s">See Detail</span></a>
						</div>
					</div>
				</div>
			</div>
			{/foreach}

		</div>
	</section>
</div>

<!-- WHY CHOOSE US -->
<section class="more-feature">
	<div class="container">
		<div class="row">
			<div style = "margin:auto;" class="col-md-12">
				<div class="more-feature-left">

					<div n:if="$about!=NULL" class="sectionTitle">
						<h2><a n:href =":Front:About:about" >{$about->title}</a></h2>
						<p style="color:#848484;" >{$about->description|noescape}</p>
					</div>
					<div class="more-features">
						<div class="feature-icon">
							<i class="fa fa-building fa-lg" aria-hidden="true"></i>
						</div>
						<div n:if="$sm_city!=NULL" class="feature-details">
							<h4><a n:href =":Front:About:subject 'smart_city'" >{$sm_city->title}</a></h4>
							<p style="color:#848484;" >{$sm_city->description|noescape}</p>
						</div>
					</div>
					<div class="more-features">
						<div class="feature-icon">
							<i class="fa fa-industry" aria-hidden="true"></i>
						</div>
						<div n:if="$sm_grid!=NULL" class="feature-details">
							<h4><a n:href =":Front:About:subject 'smart_grid'" >{$sm_grid->title}</a></h4>
							<p style="color:#848484;" >{$sm_grid->description|noescape}</p>
						</div>
					</div>
					<div class="more-features">
						<div class="feature-icon">
							<i class="fa fa-cogs" aria-hidden="true"></i>
						</div>
						<div n:if="$industry!=NULL" class="feature-details">
							<h4><a n:href =":Front:About:subject 'industry'" >{$industry->title}</a></h4>
							<p style="color:#848484;" >{$industry->description|noescape}</p>
						</div>
					</div>
				</div>
			</div>
			<!--<div class="col-md-6">
				<div class="data-chart">
					<canvas id="myChart"></canvas>
					<p style="text-align:center;">Financial statistics of the research group.</p>
				</div>
			</div>
			-->
		</div>
	</div>
</section>

<section class="countup-section bg-image" style="background-image: url('{$basePath}/images/VUT.jpg'); background-attachment: fixed;">
	<div class="container">
		<div class="row">
			<div class="col-sm-3 col-6">
				<div class="text-center single_block">
					<div class="counter">{$publications}</div><span class="count"></span>
					<h5>Publications</h5>
				</div>
			</div>
			<div class="col-sm-3 col-6">
				<div class="text-center single_block">

					<div class="counter">{$projects}</div><span class="count"></span>
					<h5>Projects</h5>
				</div>
			</div>
			<div class="col-sm-3 col-6">
				<div class="text-center single_block">

					<div class="counter">{$results}</div><span class="count"></span>
					<h5>Results</h5>
				</div>
			</div>
			<div class="col-sm-3 col-6">
				<div class="text-center single_block">

					<div class="counter">{$membersCount}</div><span class="count"></span>
					<h5>Team members</h5>
				</div>
			</div>
		</div>
	</div>
</section>

<section class="home-blog">
	<div class="container">
		<!-- section title -->
		<div class="row justify-content-center">
			<div class="col-sm-8">
				<div class="sectionTitle text-center">
					<h2>News</h2>
					<p></p>
				</div>
			</div>
		</div>
		<!-- section title ends -->
		<div class="row">
			{foreach $articles as $article}
			{if $article->draft == 1}
			<div  class="col-md-6">

				<div  class="card card-style3">
					<div class="card_img">
						<a n:href=":Front:News:detail $article->id"><img class="img-full" data-original="{$basePath}/images/posts/title/{$article->title_img}" alt="Card image"></a>
						<div>
							<span></span>
						</div>
					</div>

					<div  class="card-block posts_default">

						<a n:href=":Front:News:detail $article->id"><h4 class="card-title">{$article->title}</h4></a>

					</div>


				</div>
				<p style="color:#848484;" >{$article->description}</p>
			</div>
			{/if}
			{/foreach}

		</div>
	</div>
</section>


