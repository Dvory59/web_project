<!DOCTYPE html>
<html lang="en">
<head>

	<!-- SITE TITTLE -->
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<title>SG Group</title>

	<!-- PLUGINS CSS STYLE -->
	<link href="{$basePath}/frontend/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
	<link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.0.13/css/all.css" integrity="sha384-DNOHZ68U8hZfKXOrtjWvjxusGo9WQnrNx2sqG0tfsghAvtVlRW3tvkXWZh58N9jp" crossorigin="anonymous">
	<link href="{$basePath}/frontend/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">

	<link href="{$basePath}/frontend/plugins/selectbox/select_option1.css" rel="stylesheet">
	<link href="{$basePath}/frontend/plugins/slick/slick.css" rel="stylesheet" media="screen">
	<link href="{$basePath}/frontend/plugins/slick/slick-theme.css" rel="stylesheet" media="screen">
	<link href="{$basePath}/frontend/plugins/prismjs/prism.css" rel="stylesheet">
	<link href="{$basePath}/frontend/plugins/fancybox/jquery.fancybox.min.css" rel="stylesheet"/>
	<link href="{$basePath}/frontend/plugins/fancybox/fancyMorph.css" rel="stylesheet"/>
	<link href="{$basePath}/frontend/plugins/selectbox/select_option1.css" rel="stylesheet">
	<link href="{$basePath}/frontend/plugins/thin-line-icons/css/thin-line-icons.css" rel="stylesheet">
	<link href="{$basePath}/frontend/plugins/isotope/isotope.min.css" rel="stylesheet">
	<link href="{$basePath}/frontend/plugins/animate.css" rel="stylesheet">


	<!-- GOOGLE FONT -->

	<!-- CUSTOM CSS -->
	<link href="{$basePath}/frontend/css/style.css" rel="stylesheet">
	<link rel="stylesheet" href="{$basePath}/frontend/css/default.css" id="option_color">

	<!-- FAVICON -->
	<link href="{$basePath}/frontend/img/favicon.png" rel="shortcut icon">
	<style>
		.nav-name {
			margin-left: 30px;
		}
		@media (max-width: 1000px)
		{
			.nav-name {
				display:none;
			}
		}
	</style>
	<!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>

	<![endif]-->
	{include head}
</head>
<body id="body" class="home-classic boxed-menu">
<div class="mobile-sticky-header-overlay"></div>

<!-- TOPBAR -->
<div class="topbar">
	<div class="container">
		<div class="row">
			<div n:foreach = "$mainContact as $contact" class="col-sm-12" >
				<div class="topbar-content" style="margin-left:15px;">
					<i class="fa fa-phone" aria-hidden="true"></i>
					{$contact->phone_number}
				</div>
				<div class="topbar-content">
					<i class="fa fa-envelope" aria-hidden="true"></i>
					{$contact->email}
				</div>
				<div class="topbar-content">
					<i class="fa fa-map-marker" aria-hidden="true"></i>
					<p>{$contact->address}</p>
					{if $user->isLoggedIn()}
						<a n:href=":Admin:Default:default">Admin</a>
						<a n:href=":Admin:Sign:out">odhlásit</a>
					{/if}
				</div>
				<a href="https://www.vutbr.cz/" style="float:right; margin:0 15px 0 0;">
					<img src="{$basePath}/www/images/BUT_logo.png" style="max-height:65px;">
				</a>
				
			</div>

		
		</div>
	</div>
</div><!-- TOPBAR ENDS-->
<!-- HEADER -->
<header id="pageTop" class="header">
	<!-- NAVBAR -->
	<nav class="navbar navbar-expand-md main-nav bg-dark-navy">
		<div class="container">
			<button class="navbar-toggler navbar-toggler-right">
				<span class="burger-menu icon-toggle"><i class="fa fa-bars"></i></span>
			</button>
			<a class="navbar-brand" n:href=":Front:Default:">

				<!-- Image Logo - if you want to use jpg or png format -->
				<img src="{$basePath}/frontend/img/sg_logo.png" alt="Logo">
			</a>

			<span class="nav-name"><p style="color:white;">SG Research Group</p><p
						style="min-width: 213px;color:white;"> Brno University of Technology</p></span>
			<!-- Navigační menu -->
			<div class="navbar-collapse">
				<!--Home (prvni)-->
				<ul class="navbar-nav ml-auto">
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:Default:*') ? active">
						<a class="nav-link" n:href=":Front:Default:default"><span
									class="sr-only">(current)</span>Home</a>
					</li>
					<!-- Pages (druhy)-->
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:About:*') ? active">
						<a class="nav-link" n:href=":Front:About:about" ><span class="sr-only">(current)</span>About</a>

					</li>
					<!--Services (druhý) -->
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:News:*') ? active">
						<a class="nav-link" n:href=":Front:News:list"><span class="sr-only">(current)</span>News</a>
					</li>
					<!--Portfolio (třetí) -->
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:Research:*') ? active">
						<a class="nav-link" n:href=":Front:Research:list"><span class="sr-only">(current)</span>Research</a>

					</li>
					<!--Blogs (čtvrtý)-->
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:Team:*') ? active">
						<a class="nav-link" n:href=":Front:Team:team"><span class="sr-only">(current)</span>Team</a>

					</li>
					<li n:class ="nav-item, $presenter->isLinkCurrent(':Front:Contact:*') ? active">
						<a class="nav-link" n:href=":Front:Contact:"><span class="sr-only">(current)</span>Contact</a>

					</li>

					</li>

					<!-- header search -->

				</ul>
			</div>
			<!-- header search ends-->
		</div><!-- NAVBAR ENDS-->
	</nav>
</header>

<!-- Hlavní obsah stránek-->
<div class="main-wrapper home">
	{ifCurrent Default:*}

	{else}
	<section class="bredcrumb">
		<div class="bg-image text-center" style="background-image: url('{$basePath}/images/VUT.jpg');">
			{/ifCurrent}
			<!--Dynamický obsah-->
			{include content}

<!-- Patička-->
			<footer class="footer footer-classic bg-navy">
				<div class="container">
					<div class="row">
						<div class="col-md-3">
							<div class="footer-text">
								<a href="">
									<img src="{$basePath}/frontend/img/sg_logo.png" alt="Logo" class="footer-logo"
										 height="70px">
								</a>
								<p>
									{$aboutFooter->description}
								</p>
								{foreach $mainContact as $contact}
								<div class="social-icons">
									<a n:if="$contact->research_gate != NULL" href="https://{$contact->research_gate}" class="btn btn-social btn-social-o">
										<i class="fab fa-researchgate"></i>
									</a>
									<a n:if="$contact->twitter!= NULL" href="https://{$contact->twitter}" class="btn btn-social btn-social-o twitter">
										<i class="fa fa-twitter"></i>
									</a>
									<a n:if="$contact->linkedin != NULL" href="https://{$contact->linkedin}" class="btn btn-social btn-social-o linkedin">
										<i class="fa fa-linkedin"></i>
									</a>
									<a n:if = "$contact->facebook != NULL" href="https://{$contact->facebook}" class="btn btn-social btn-social-o facebook">
										<i class="fa fa-facebook-f"></i>
									</a>
								</div>
								{/foreach}
							</div>
						</div>
						<div class="col-xl-2 col-md-2">
							<div class="links">
								<h3>Links</h3>
								<ul class="">
									<li><a n:href=":Front:About:about">About</a></li>
									<li><a n:href=":Front:News:list">News</a></li>
									<li><a n:href=":Front:Research:list">Research</a></li>
									<li><a n:href=":Front:Team:team">Team</a></li>
									<li><a n:href=":Admin:Default:default">Admin</a></li>
								</ul>
							</div>
						</div>
						<div class="col-md-3">
							<div class="location" n:foreach = "$mainContact as $contact">
								<h3>Location</h3>
								<ul>
									<li style="margin-left:20px;"><i class="fa fa-home"></i> {$contact->address}</li>
									<li><i class="fa fa-phone"></i> {$contact->phone_number}</li>
									<li><i class="fa fa-envelope"></i>{$contact->email}</li>
								</ul>
							</div>
						</div>


						<!-- BACK TO TOP BUTTON -->
						<!-- <a href="#pageTop" class="backToTop"><i class="fa fa-angle-up" aria-hidden="true"></i></a> -->

						<!-- COPY RIGHT -->
					</div>
					<div class="copyright">
						<hr>

						<div class="row justify-content-center">
							<div class="col-sm-6">
								<div class="copyRight_text text-center">
									<p> &copy; 2017 Copyright Brno University of technology.
									</p>
								</div>
							</div>
						</div>
					</div>
				</div>
			</footer>
		</div>


		<!-- JAVASCRIPTS -->
		<script src="{$basePath}/frontend/plugins/jquery/jquery.min.js"></script>
		<script src="{$basePath}/frontend/plugins/jquery/jquery-migrate-3.0.0.min.js"></script>
		<script src="{$basePath}/frontend/plugins/bootstrap/js/tether.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js"
				integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4"
				crossorigin="anonymous"></script>
		<script src="{$basePath}/frontend/plugins/bootstrap/js/bootstrap.min.js"></script>
		<script src="{$basePath}/frontend/plugins/selectbox/jquery.selectbox-0.1.3.min.js"></script>
		<script src="{$basePath}/frontend/plugins/slick/slick.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.6.0/Chart.min.js"></script>
		<script src="{$basePath}/frontend/plugins/circle-progress/jquery.appear.js"></script>
		<script src="{$basePath}/frontend/plugins/isotope/isotope.min.js"></script>
		<script src="{$basePath}/frontend/plugins/lazyload/lazyload.min.js"></script>
		<script src="{$basePath}/frontend/plugins/fancybox/jquery.fancybox.min.js"></script>
		<script src="{$basePath}/frontend/plugins/fancybox/fancyMorph.js"></script>
		<script src="http://cdnjs.cloudflare.com/ajax/libs/waypoints/2.0.3/waypoints.min.js"></script>
		<script src="{$basePath}/frontend/plugins/counterUp/jquery.counterup.js"></script>
		<script src="{$basePath}/frontend/plugins/smoothscroll/SmoothScroll.js"></script>
		<script src="{$basePath}/frontend/plugins/syotimer/jquery.syotimer.min.js"></script>
		<script src="{$basePath}/frontend/js/custom.js"></script>
		{include footer}
</body>
</html>
